<div class="container">
  <div class="panel panel-primary">
      <div class="panel-heading"> Search Results for <b>"{{data.keywords}}"</b></div>
      <div class="panel-body">
        <p></p>
        <!-- Table -->
        <table class="table table striped">

          <tr>
                     <th>
                         Name
                     </th>
                     <th>
                         Vendor
                     </th>
                     <th>
                         Price
                     </th>
                     <th>
                         Shop
                     </th>
                 </tr>
                     {{#each data.products}}
                 <tr>
                     <td>
                         {{title}}
                     </td>
                      <td>
                         {{vendor}}
                      </td>
                      <td>
                         {{price}}
                      </td>
                      <th>
                          {{url}}
                      </th>
                 </tr>
                         {{/each}}

        </table>

  </div>

</div>

</div>
